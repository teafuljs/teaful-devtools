{
  "name": "teaful-devtools",
  "version": "0.2.0",
  "description": "Browser extension for inspection Teaful applications",
  "license": "MIT",
  "keywords": [
    "teaful",
    "react",
    "preact",
    "state",
    "state management",
    "store",
    "javascript"
  ],
  "author": {
    "name": "Aral Roca GÃ²mez",
    "email": "contact@aralroca.com"
  },
  "files": [
    "dist",
    "bridge"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/teafuljs/teaful-devtools.git"
  },
  "source": "bridge/index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./dist/index.js",
        "maxSize": "300 B"
      },
      {
        "path": "./dist/index.modern.js",
        "maxSize": "300 B"
      },
      {
        "path": "./dist/index.m.js",
        "maxSize": "300 B"
      },
      {
        "path": "./dist/index.umd.js",
        "maxSize": "300 B"
      }
    ]
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "microbundle",
    "build:chrome-extension": "cd chrome-extension/panel && yarn build && cd ../..",
    "dev": "microbundle watch",
    "prepublish": "yarn build"
  },
  "peerDependencies": {
    "teaful": ">= 0.9.0"
  },
  "devDependencies": {
    "bundlewatch": "0.3.2",
    "microbundle": "0.14.1"
  },
  "bugs": "https://github.com/teafuljs/teaful-devtools/issues"
}